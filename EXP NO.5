#include <stdio.h>
#include <time.h>

int factorial(int n) {
    int result = 1;
    for (int i = 1; i <= n; i++) {
        result *= i;
    }
    return result;
}

int main() {
    int n;
    printf("Enter a positive integer: ");
    scanf("%d", &n);

    clock_t start = clock();
    int result = factorial(n);
    clock_t end = clock();
    double time_taken = ((double)(end - start)) / CLOCKS_PER_SEC;

    printf("The factorial of %d is %d\n", n, result);
    printf("Time taken to compute the factorial is: %f\n", time_taken);
    return 0;
}
